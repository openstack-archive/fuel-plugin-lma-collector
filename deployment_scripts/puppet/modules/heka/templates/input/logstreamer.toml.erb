log_directory = "<%= @log_directory %>"
file_match = '<%= @file_match %>'
<% if @differentiator -%>
differentiator = <%= @differentiator %>
<% end -%>
decoder = "<%= @decoder %>_decoder"
splitter = "<%= @splitter ? "#{ @splitter }_splitter" : "TokenSplitter" %>"

[hekad]
maxprocs=<%= @maxprocs %>
base_dir="<%= @base_dir %>"
<% if @hostname %>
hostname="<%= @hostname %>"
<% end %>
<% if @max_message_size -%>
max_message_size = <%= @max_message_size %>
<% end -%>
<% if @max_process_inject -%>
max_process_inject = <%= @max_process_inject %>
<% end -%>
<% if @max_timer_inject -%>
max_timer_inject = <%= @max_timer_inject %>
<% end -%>
<% if @poolsize -%>
poolsize = <%= @poolsize %>
<% end -%>

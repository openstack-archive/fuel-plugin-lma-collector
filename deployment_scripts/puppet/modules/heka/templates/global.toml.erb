[hekad]
maxprocs=<%= @_maxprocs %>
base_dir="<%= @base_dir %>"
<% if @_hostname %>
hostname="<%= @_hostname %>"
<% end %>
<% if @_max_message_size -%>
max_message_size = <%= @_max_message_size %>
<% end -%>
<% if @_max_process_inject -%>
max_process_inject = <%= @_max_process_inject %>
<% end -%>
<% if @_max_timer_inject -%>
max_timer_inject = <%= @_max_timer_inject %>
<% end -%>
<% if @poolsize -%>
poolsize = <%= @poolsize %>
<% end -%>

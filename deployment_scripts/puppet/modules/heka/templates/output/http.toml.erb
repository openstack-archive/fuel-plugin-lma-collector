[<%= @title %>_output]
type = "HttpOutput"
message_matcher = "<%= @message_matcher %>"
encoder = "<%= @encoder %>_encoder"
address = "<%= @url %>"
<% if @username -%>
username = "<%= @username %>"
<% end -%>
<% if @password -%>
password = "<%= @password %>"
<% end -%>
<% unless @timeout.nil? -%>
http_timeout = <%= @timeout * 1000 %>
<% end -%>
method = "<%= @method %>"

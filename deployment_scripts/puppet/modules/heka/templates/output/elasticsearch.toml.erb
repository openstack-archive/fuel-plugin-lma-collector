[<%= @title %>_output]
type = "ElasticSearchOutput"
message_matcher = "<%= @message_matcher %>"
encoder = "<%= @encoder %>_encoder"
<% # Heka expects milliseconds here -%>
flush_interval = <%= @flush_interval.to_i() * 1000 %>
flush_count = <%= @flush_count %>
server = "http://<%= @server %>:<%= @port %>"

[<%= @title %>_encoder]
type = "SandboxEncoder"
filename = "<%= @filename %>"
<% if @module_directory -%>
module_directory = "<%= @module_directory %>;/usr/share/heka/lua_modules"
<% end -%>
<% if @config.size() > 0 %>
[<%= @title %>_encoder.config]
<% @config.each do |k,v| -%>
<%- next if v.nil? or v == :undef -%>
    <%= k %> = <%= v.is_a?(String) ? "'#{v}'" : v %>
<%  end %>
<% end %>
